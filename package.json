  },
  "homepage": "https://github.com/kauppfbi/vuepress-starter#readme",
  "devDependencies": {
    "gh-pages": "2.1.1",
    "husky": "3.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "vuepress": "1.0.3"
  },
  "husky": {
